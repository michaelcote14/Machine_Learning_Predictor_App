MainMenu:   # this chooses which interface or page to open


<MainMenu>:
    orientation: 'vertical'
    Label:
        text: "Main Menu"
        size_hint: 1, .1   # (x, y) - percentages. because its in box layout, you can't use size unless you use this line
    Options:




<Options>:
    padding: ('40dp', '40dp', '40dp', '40dp')
    spacing: "10dp" # this puts space between all the below elements

    Button:
        text: 'Model Tester'
        size_hint: 1/3, 1/3
    Button:
        text: 'Saved Data'
        size_hint: 1/3, 1/3
    Button:
        text: 'Model Runner'
        size_hint: 1/3, 1/3
    Button:
        text: 'Logs'
        size_hint: 1/3, 1/3
    Button:
        text: 'Cleaning Tester'
        size_hint: 1/3, 1/3







<ModelRunner>:
# at least one of rows and cols below are mandatory arguments
    #rows: 5
    cols: 2
    Button:
        text: 'A'
    BoxLayoutExample:
    Button:
        text: "B"
        size_hint:
    BoxLayoutExample:
    Button:
        text: 'D'
        size_hint: .5, 1
    BoxLayoutExample:
    Button:
        text: 'F'
        size_hint: .5, 1
    BoxLayoutExample:
    Button:
        text: 'E'
        size_hint: .5, 1
    BoxLayoutExample:
    Button:
        text: 'I'
        size_hint: .5, 1
    BoxLayoutExample:

<BoxLayoutExample>
<BoxLayoutExample>:
    orientation: "vertical"  # this changes the axis of your page
    Button:
        text: "1"
    ToggleButton:
        text: "Start"
        on_state: root.on_toggle_button_state(self)
    ProgressBar:
        max: 100 # where the progress bar stops
        value: 0 # where the progress bar starts

